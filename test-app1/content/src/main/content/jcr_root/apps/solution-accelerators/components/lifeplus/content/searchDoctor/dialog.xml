<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="SearchDoc"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Search Doctor">
                <items jcr:primaryType="cq:WidgetCollection">
                    <docName
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Doctors Name"
                        name="./docName"
                        xtype="searchfield"/>
                    <speciality
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Speciality"
                        name="./speciality"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <Neurologist
                                jcr:primaryType="nt:unstructured"
                                text="Neurologist"
                                value="neurologist"/>
                            <Ophthalmologist
                                jcr:primaryType="nt:unstructured"
                                text="Opthalmologist"
                                value="opthalmologist"/>
                            <Dermatologist
                                jcr:primaryType="nt:unstructured"
                                text="Dermatologist"
                                value="dermatologist"/>
                            <Heart_x0020_Specialist
                                jcr:primaryType="nt:unstructured"
                                text="Heart Specialist"
                                value="heart Specialist"/>
                            <cardio jcr:primaryType="nt:unstructured"/>
                        </options>
                    </speciality>
                    <location
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Location *( ZipCode,City,State)"
                        name="./location"
                        xtype="textfield"/>
                    <near
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Near Me Now"
                        name="./near"
                        xtype="checkbox"/>
                    <date
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Date"
                        name="./date"
                        xtype="datefield"/>
                    <consultation jcr:primaryType="cq:Widget"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
