<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                id="tab1"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <member
                        jcr:primaryType="cq:Widget"
                        default="0"
                        fieldLabel="Member"
                        id="member"
                        name="./member_id"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <not_logged_in
                                jcr:primaryType="nt:unstructured"
                                name="Not Logged In"
                                value="0"/>
                            <member1
                                jcr:primaryType="nt:unstructured"
                                name="1"
                                value="1"/>
                            <member2
                                jcr:primaryType="nt:unstructured"
                                name="2"
                                value="2"/>
                            <member3
                                jcr:primaryType="nt:unstructured"
                                name="3"
                                value="3"/>
                            <member4
                                jcr:primaryType="nt:unstructured"
                                name="4"
                                value="4"/>
                            <member5
                                jcr:primaryType="nt:unstructured"
                                name="5"
                                value="5"/>
                            <member6
                                jcr:primaryType="nt:unstructured"
                                name="6"
                                value="6"/>
                            <member7
                                jcr:primaryType="nt:unstructured"
                                name="7"
                                value="7"/>
                            <member8
                                jcr:primaryType="nt:unstructured"
                                name="8"
                                value="8"/>
                            <member9
                                jcr:primaryType="nt:unstructured"
                                name="9"
                                value="9"/>
                            <member10
                                jcr:primaryType="nt:unstructured"
                                name="10"
                                value="10"/>
                        </options>
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            selectionchanged="function(field,value,isChecked) { &#xa;&#x9;field.findParentByType('tabpanel').findById(&quot;tab1&quot;).findById(&quot;member&quot;).value = value;&#xa;}"/>
                    </member>
                    <interest
                        jcr:primaryType="cq:Widget"
                        default="general"
                        fieldLabel="Interest"
                        id="interest"
                        name="./interest"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <general
                                jcr:primaryType="nt:unstructured"
                                name="general"
                                value="general"/>
                            <doctor
                                jcr:primaryType="nt:unstructured"
                                name="doctor"
                                value="doctor"/>
                            <hospital
                                jcr:primaryType="nt:unstructured"
                                name="hospital"
                                value="hospital"/>
                            <pharmacy
                                jcr:primaryType="nt:unstructured"
                                name="pharmacy"
                                value="pharmacy"/>
                        </options>
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            selectionchanged="function(field,value,isChecked) { &#xa;&#x9;field.findParentByType('tabpanel').findById(&quot;tab1&quot;).findById(&quot;interest&quot;).value = value;&#xa;}"/>
                    </interest>
                    <testimonial_count
                        jcr:primaryType="cq:Widget"
                        default="1"
                        fieldLabel="Testimonials Count"
                        id="testi_count"
                        name="./testi_count"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <count_1
                                jcr:primaryType="nt:unstructured"
                                name="1"
                                value="1"/>
                            <count_2
                                jcr:primaryType="nt:unstructured"
                                name="2"
                                value="2"/>
                            <count_3
                                jcr:primaryType="nt:unstructured"
                                name="3"
                                value="3"/>
                            <count_4
                                jcr:primaryType="nt:unstructured"
                                name="4"
                                value="4"/>
                            <count_5
                                jcr:primaryType="nt:unstructured"
                                name="5"
                                value="5"/>
                            <count_6
                                jcr:primaryType="nt:unstructured"
                                name="6"
                                value="6"/>
                            <count_7
                                jcr:primaryType="nt:unstructured"
                                name="7"
                                value="7"/>
                            <count_8
                                jcr:primaryType="nt:unstructured"
                                name="8"
                                value="8"/>
                            <count_9
                                jcr:primaryType="nt:unstructured"
                                name="9"
                                value="9"/>
                            <count_10
                                jcr:primaryType="nt:unstructured"
                                name="10"
                                value="10"/>
                        </options>
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            selectionchanged="function(field,value,isChecked) { &#xa;&#x9;field.findParentByType('tabpanel').findById(&quot;tab1&quot;).findById(&quot;testi_count&quot;).value = value;&#xa;}"/>
                    </testimonial_count>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
